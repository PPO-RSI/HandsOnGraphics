<RSIControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="PPO170117_2.TrendLabel"
	Background="LightGray">
    <RSIGrid>
        <RSIStackPanel Orientation="Horizontal" Grid.Row="2">
            <RSIRectangle Width="20" Height="20" Margin="10,0">
                <RSIRectangle.Animations>
                    <Animation>
                        <ChangePropertyValue PropertyName="Fill" Value="$COLOR"/>
                    </Animation>
                </RSIRectangle.Animations>
            </RSIRectangle>
            <RSIText Text="NAME" MinWidth="80" Margin="10,0" Height="20" TextAlignment="Center" FontSize="16" ></RSIText>
            <RSIText Text="MIN"  MinWidth="80" Margin="10,0" Height="20" TextAlignment="Center" FontSize="16"></RSIText>
            <RSIText Text="MAX"  MinWidth="80" Margin="10,0" Height="20" TextAlignment="Center" FontSize="16"></RSIText>
            <RSIText Text="CURRENT" MinWidth="80" Margin="10,0" Height="20" TextAlignment="Center" FontSize="16"></RSIText>
            <RSIText Text="CURSOR" Margin="10,0" Height="20" TextAlignment="Center" FontSize="16"></RSIText>
           
        </RSIStackPanel>
    </RSIGrid>
</RSIControl>